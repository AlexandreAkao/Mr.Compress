<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="estruturaav3v4.TelaControle">
    <children>
        <HBox alignment="CENTER" layoutX="9.0" layoutY="14.0" prefHeight="44.0" prefWidth="287.0" spacing="10.0">
            <children>
                <Button mnemonicParsing="false" onAction="#compactar" text="Compactar aqui">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
                <Button mnemonicParsing="false" onAction="#descompactar" text="Descompactar aqui">
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Button>
            </children>
        </HBox>
        <Button layoutX="132.0" layoutY="111.0" mnemonicParsing="false" onAction="#sair" text="Sair">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
      <HBox alignment="CENTER" layoutX="9.0" layoutY="58.0" prefHeight="44.0" prefWidth="287.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#compactarPara" text="Compactar para...">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button mnemonicParsing="false" onAction="#descompactarPara" text="Descompactar para...">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
    </children>
</AnchorPane>
